@misc{Strain.2017,
abstract = {This study examined the difference in thriving levels in a sample of 346 community college students at one institution compared to the national norms and determined the degree to which peer support contributes to the variance in scores on the Thriving Quotient (Schreiner, 2012). Thriving is defined as full engagement in college experiences on an intellectual, psychological, and interpersonal level and contains 5 subscales of Engaged Learning, Academic Determination, Positive Perspective, Social Connectedness, and Diverse Citizenship (Schreiner, 2010, 2016). One-sample t tests revealed this community college sample was thriving at significantly higher levels than the national 4-year college norms on all subscales of thriving. Hierarchical multiple regression analyses of the predictors of overall thriving and scores on each subscale indicated that 48{\%} of the variance in thriving could be explained by the full model, with student-faculty interaction and institutional integrity as the largest contributors to the variance in thriving. As hypothesized, peer support contributed significantly to the variation in thriving, adding a unique 6{\%} to the variation in Thriving Quotient scores after accounting for student demographic characteristics and other campus experiences. Peer support contributed the most to the variance in the Social Connectedness scale (change in R2 = .20) and also significantly to the variance in Engaged Learning and Diverse Citizenship scale scores. Peer support did not contribute significantly to the variance in Academic Determination or Positive Perspective scores, however. Given these findings, implications for practice at community colleges that can aid this population of students to thrive include providing opportunities on campus to develop peer support through peer-to-peer mentoring. Additionally, encouraging positive interactions between faculty and students through a faculty-to-peer mentoring program could allow meaningful processing outside the classroom and provide additional avenues for students to make connections on the community college campus. Finally, institutional integrity can be improved by ensuring the institution communicates clearly, that it sets appropriate expectations, and that faculty and staff interact with students in ways that are congruent with the mission and values of the institution.},
author = {Strain, Cassandra},
keywords = {Community College Education;Community Collge;EDUCA},
publisher = {ProQuest Dissertations Publishing},
title = {{Thriving in the Community College: The Contribution of Peer Support, Student-Faculty Interaction, and Institutional Integrity}},
year = {2017}
}
