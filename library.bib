@article{Ferguson.2005,
abstract = {This article focuses on radio personality Kate Aiken. Bob Hortop found his future mother-in-law intimidating when he first met her in 1946 -- although not for the usual reasons. She was Kate Aitken. On three daily radio shows over three decades, in the pages of the Montreal Standard during the 1940s and as a cooking-school and fashion-show host in cities across the country, Canada's doyenne of domesticity ruled good-naturedly, yet firmly, on everything from keeping fruitcakes moist to refashioning worn-out clothes. The Beeton, Ont., shopkeeper's daughter first gained fame in the early 1920s for her thriving poultry farm and canning business, as well as for her work in the local Women's Institute, which she helped found. Never questioning a woman's duty to her husband and family, Aitken -- who died in 1971 at age 81 -- wouldn't pass muster with everyone celebrating International Women's Day this March 8. Aitken launched her radio career literally by accident. Her later broadcasts usually led with cooking tips, but quickly moved on to more serious fare, treating her audience to firsthand accounts of a 1938 speech by Hitler in Berlin, Hungarian refugees fleeing their strife-torn country in 1956 and countless other newsmaking events. Despite shuttling between Montreal and Toronto weekly, Aitken found time to oversee her farm's annual production of 12,000 jars of preserves, launch 43 {\{}$\backslash$textquotedbl{\}}remake centres{\{}$\backslash$textquotedbl{\}} across Canada as wartime supervisor of clothing conservation, and write seven books. Aitken's strength of character, according to son-in-law Hortop, had little to do with being a feminist.},
author = {Ferguson, Sue},
issn = {00249262},
journal = {Maclean's},
keywords = {AIKEN;CANADA;CELEBRITIES;FEMINISM;Kate;RADIO progr},
number = {10},
pages = {49},
title = {{FROM HEARTH TO HITLER}},
volume = {118},
year = {2005}
}
